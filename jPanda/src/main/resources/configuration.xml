		<typeAlias alias="RegistTalent" type="com.kakao.jPanda.bsm.domain.Talent"/>    	
      
		<!-- 채팅 -->
		<typeAlias type="com.kakao.jPanda.kts.domain.Chat" alias="Chat"/>
		<typeAlias type="com.kakao.jPanda.kts.domain.ChatMessage" alias="ChatMessage"/>
		<typeAlias type="com.kakao.jPanda.kts.domain.Partner" alias="ChatPartner"/>
		<typeAlias type="com.kakao.jPanda.kts.domain.Member" alias="ChatMember"/>

  	</typeAliases>
    
</configuration>