  		<typeAlias alias="TalentRefund" type="com.kakao.jPanda.yjh.domain.TalentRefundDto"/>
  		
  		<!-- 로그인, 회원가입, 마이페이지 -->
		<typeAlias alias="MemberDto" type="com.kakao.jPanda.njb.domain.MemberDto"/>
  		<typeAlias alias="BankDto" type="com.kakao.jPanda.njb.domain.BankDto"/>
  		
  		<!-- 판매게시판 -->
		<typeAlias alias="BoardTalent" type="com.kakao.jPanda.lhw.domain.Talent"/>
  		<typeAlias alias="BoardNotice" type="com.kakao.jPanda.lhw.domain.Notice"/>
  		<typeAlias alias="BoardCategory" type="com.kakao.jPanda.lhw.domain.Category"/>
