  		<typeAlias alias="BoardFilters" type="com.kakao.jPanda.lhw.domain.Filters"/>
  		
  		<!-- 충전 -->
  		<typeAlias alias="ChargeDto" type="com.kakao.jPanda.kyg.domain.ChargeDto"/>
		<typeAlias alias="ChargeCouponUseDto" type="com.kakao.jPanda.kyg.domain.CouponUseDto"/>
		<typeAlias alias="ChargeCouponDto" type="com.kakao.jPanda.kyg.domain.CouponDto"/>
		
		<!-- 판매등록, 메인페이지 -->
		<typeAlias alias="RegistCategory" type="com.kakao.jPanda.bsm.domain.Category"/>
